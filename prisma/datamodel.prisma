type User {
    id: ID! @id
    createdAt: DateTime! @createdAt
    username: String!
    password: String!
    usertype: String!
    email: String!
    properties: [Property!]
}

type Property {
    id: ID! @id
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
    address: String!
    owner: User
}